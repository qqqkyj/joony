package day0620;

public class ForTest_16 {

	public static void main(String[] args) {
		//1.1~5까지 출력하시오
		for(int i=1; i<=5; i++) //for(begin, 조건, 증감식)
		{
			System.out.printf(i+" ");
		}
		
		System.out.println("\n");
		
		for(int i=5; i>=1; i--) //for(begin, 조건, 증감식)
		{
			System.out.printf(i+" ");
		}
	}

}
