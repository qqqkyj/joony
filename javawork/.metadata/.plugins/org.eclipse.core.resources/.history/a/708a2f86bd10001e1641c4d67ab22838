package day0622;

public class ExForGugu_04 {

	public static void main(String[] args) {
		//2~9단까지 구구단=>중첩for문
		
		/*for(int i=1; i<=9; i++) {// 가로출력
			for(int j=2; j<=9; j++) {
				System.out.printf(j+"X"+i+"="+j*i+"\t");
			}
			System.out.println(" ");
		}*/
		
		
		//구구단으로 break연습
		for(int i=2; i<=9; i++) {//세로 출력
			for(int j=1; j<=9; j++) {
				if(j==5)
					break;
				System.out.println(i+"X"+j+"="+i*j);
			}
			System.out.println(" ");
		}
		
	}

}
