package day0703;

import java.util.HashMap;
import java.util.Map;

public class Map_14 {
	
	public static void main(String[] args) {
		//키는 중복을 허용하지 않고, 값은 중복을 허용
		//key와 value를 쌍으로 저장
		Map<String, String> map = new HashMap<String, String>();
		map.put("name", "손석구");
		map.put("age", "25");
		map.put("addr", "제주시 애월읍");
		
		//같은 키값으로 넣으면 먼저 데이터는 없어짐
		
		
	}

}
