package day0622;

public class ExForGugu_04 {

	public static void main(String[] args) {
		//2~9단까지 구구단=>중첩for문
		
		for(int i=1; i<=9; i++) {
			for(int j=2; j<=9; j++) {
				System.out.printf(j+"X"+i+"="+j*i+"\t");
			}
			System.out.println(" ");
		}
		
		for(int i=2; i<=9; i++) {
			for(int j=1; j<=9; j++) {
				System.out.printf(i+"X"+j+"="+i*j+" ");
			}
			System.out.println(" ");
		}
		
	}

}
