package day0626;

public class ReviewArr_08 {

	public static void main(String[] args) {
		//해당 배열을 for문을 이용하여 출력하시오
		
		//1.ex)0번지: 영환이
		String[] names = {"영환이","수환이","동환이","성환이"};
		for(int i=0; i<names.length; i++) {
			System.out.println(names[i]+" ");
		}
		
		
		//2.해당 배열을 가로로 출력
		int [] nums = {2,4,5,7,8};
		
		
		//flowers
		
		
		//colors
		
		
	}

}
