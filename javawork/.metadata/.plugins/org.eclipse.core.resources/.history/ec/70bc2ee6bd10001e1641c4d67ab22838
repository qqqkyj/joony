package day0622;

public class ExForGugu_04 {

	public static void main(String[] args) {
		//2~9단까지 구구단=>중첩for문
		
		/*for(int i=1; i<=9; i++) {// 가로출력
			for(int j=2; j<=9; j++) {
				System.out.printf(j+"X"+i+"="+j*i+"\t");
			}
			System.out.println(" ");
		}*/
		
		
		//구구단으로 break연습
		//반복문앞에 이름을 붙이고 해당이름을 break처리하면 두개이상의 반복문을 빠져나가는게 가능
		
		for(int dan=2; dan<=9; dan++) {//세로 출력
			for(int j=1; j<=9; j++) 
			{
				if(j==5)
					break;
				System.out.println(dan+"X"+j+"="+dan*j);
			}
			System.out.println(" ");
		}
		
	}

}
